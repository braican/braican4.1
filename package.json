    "babel-eslint": "^10.x.x",
    "clean-css": "^4.2.3",
    "eslint": "^6.x.x",
    "eslint-config-prettier": "^6.x.x",
    "markdown-it": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.x.x",
    "sass": "^1.29.0"
  }
}
